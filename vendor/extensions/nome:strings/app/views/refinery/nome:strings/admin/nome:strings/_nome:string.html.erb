<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(nome:string) -%>">
  <span class='title'>
    <%= nome:string.modelo %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.nome:strings_nome:string_path(nome:string),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_nome:strings_admin_nome:string_path(nome:string),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.nome:strings_admin_nome:string_path(nome:string),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => nome:string.modelo),
        :method => :delete %>
  </span>
</li>
